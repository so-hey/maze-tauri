{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "maze",
  "version": "0.1.0",
  "identifier": "com.sohey.mazegame",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "width": 1000,
        "height": 750,
        "title": "MAximiZE",
        "resizable": true,
        "fullscreen": false,
        "maximized": true,
        "minimizable": true,
        "maximizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
